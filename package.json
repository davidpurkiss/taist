{
  "name": "taist",
  "version": "1.0.0",
  "description": "Token-Optimized Testing Framework for AI-Assisted Development",
  "main": "index.js",
  "type": "module",
  "bin": {
    "taist": "./taist.js"
  },
  "scripts": {
    "test": "vitest",
    "test:ai": "node taist.js test --format toon",
    "test:watch": "node taist.js watch",
    "test:trace": "node taist.js test --trace --depth 3",
    "demo": "node taist.js test -f ./examples/calculator.js -t ./examples/calculator.test.js"
  },
  "keywords": [
    "testing",
    "ai",
    "tdd",
    "vitest",
    "token-optimized",
    "llm",
    "claude",
    "copilot"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "vitest": "^1.0.4",
    "chokidar": "^3.5.3",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@vitest/ui": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
